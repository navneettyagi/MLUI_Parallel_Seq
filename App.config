    <add key="SendEmailReport" value="true" />
    <add key="EmailGroup" value="abcd@bhavnacorp.com,xyz@bhavnacorp.com" />
    <add key="SMTPHost" value="clientmail.mlsoft.com" />
    <add key="SMTPPort" value="25" />
